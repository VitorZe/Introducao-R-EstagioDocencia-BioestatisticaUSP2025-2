set.seed(42)
id <- 1:50
sexo <- sample(c("F", "M"), size = 50, replace = TRUE, prob = c(0.6, 0.4))
idade <- round(rnorm(50, mean = 22, sd = 2))
# peso e altura com alguns outliers
peso <- round(rnorm(50, mean = 65, sd = 12), 1)
peso[sample(1:50, 2)] <- peso[sample(1:50, 2)] * 2  # erro de digitação
altura <- round(rnorm(50, mean = 1.70, sd = 0.1), 2)
altura[sample(1:50, 2)] <- altura[sample(1:50, 2)] / 2  # altura estranha
# energia e proteína também "ruidosas"
energia_kcal <- round(rnorm(50, mean = 2200, sd = 400))
energia_kcal[sample(1:50, 2)] <- energia_kcal[sample(1:50, 2)] * 3
proteina_g <- round(rnorm(50, mean = 80, sd = 15))
proteina_g[sample(1:50, 2)] <- c(5, 300)  # extremos
# montar data.frame
df <- data.frame(id, sexo, idade, peso, altura, energia_kcal, proteina_g)
# salvar como csv
write.csv(df, "dadosNutri.csv", row.names = FALSE)
